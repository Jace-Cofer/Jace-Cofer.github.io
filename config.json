  "data": "data.json",
  "logo": {
    "file": "",
    "link": "",
    "text": "Jace Cofer"
  },
  "text": {
    "more": "This data has been collected across aproximately 2,000 randomly selected actors via their 'known for' films on IMBd. I was able to collect data from IMDb non-commercial data sets (https://developer.imdb.com/non-commercial-datasets/), organize it using EXcel and graph it using a program for network graphs called Gephi. ",
    "intro": "",
    "title": "Actors Who Have Been in Movies with Other Actors"
  },
  "legend": {
    "edgeLabel": "Has Been In A Movie With",
    "colorLabel": "Specialization Groups",
    "nodeLabel": "Actor Name"
  },
  "features": {
    "search": true,
    "groupSelectorAttribute": "Label",
    "hoverBehavior": "dim"
  },
  "informationPanel": {
    "groupByEdgeDirection": false,
    "imageAttribute": "Label"
  },
  "sigma": {
    "drawingProperties": {
      "defaultEdgeType": "curve",
      "defaultHoverLabelBGColor": "#002147",
      "defaultLabelBGColor": "#ddd",
      "activeFontStyle": "bold",
      "defaultLabelColor": "#ffffff",
      "labelThreshold": 10,
      "defaultLabelHoverColor": "#fff",
      "fontStyle": "bold",
      "hoverFontStyle": "bold",
      "defaultLabelSize": 14
    },
    "graphProperties": {
      "maxEdgeSize": 0.5,
      "minEdgeSize": 0.2,
      "minNodeSize": 1,
      "maxNodeSize": 7
    },
    "mouseProperties": {
      "maxRatio": 20,
      "minRatio": 0.75
    }
  }
